{
  "name": "eddtr",
  "version": "1.0.0",
  "author": "rovnyart",
  "license": "MIT",
  "scripts": {
    "start": "nodemon server --watch server --ignore migrations --exec \"npx babel-node --ignore node_modules\" --logLevel=debug",
    "start:production": "yarn run build && cd build && NODE_ENV=production node server",
    "build:app": "NODE_ENV=production webpack --mode production --color -p --progress --hide-modules --config webpack.prod.js",
    "build:server": "NODE_ENV=production npx babel server --out-dir build/server --copy-files",
    "build": "rm -fr build && yarn run build:server && yarn run build:app",
    "migrate": "npx babel-node server/database/migrate",
    "deploy": "pm2 deploy ecosystem.config.js production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --ignore-path .gitignore --ignore-pattern migrations --color ."
    }
  },
